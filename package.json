{
  "name": "sfdx-kgo-plugin",
  "description": "SFDX plugin of great daily use tools",
  "version": "0.0.9",
  "author": "KGO",
  "bugs": "https://github.com/KevinGossentCap/sfdx-kgo-plugin/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^2",
    "@salesforce/core": "^2",
    "lodash": "^4.17.20",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.4.1",
    "@types/chai": "^4",
    "@types/mocha": "^8",
    "@types/node": "^12",
    "chai": "^4",
    "globby": "^11",
    "mocha": "^8",
    "nyc": "^15",
    "rimraf": "^3.0.2",
    "ts-node": "^9",
    "tslint": "^6",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12.16.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/KevinGossentCap/sfdx-kgo-plugin",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "KevinGossentCap/sfdx-kgo-plugin",
  "scripts": {
    "build": "rimraf lib && tsc -b",
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
